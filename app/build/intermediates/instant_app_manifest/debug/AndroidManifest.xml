<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sendproperties.ws"
    android:targetSandboxVersion="2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="8" />

    <uses-permission android:name="android.permission.PERSISTENT_ACTIVITY" />
    <uses-permission android:name="android.permission.WRITE_OWNER_DATA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>

    <application
        android:debuggable="true"
        android:icon="@drawable/logobox"
        android:label="@string/app_name"
        android:testOnly="true" >
        <activity
            android:name="com.sendproperties.ws.Login"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />

                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sendproperties.ws.Productos"
            android:label="@string/title_activity_productos"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.sendproperties.ws.InsertarProducto"
            android:label="@string/title_activity_insertar_producto"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.sendproperties.ws.ModificarProducto"
            android:label="@string/title_modificar_producto" >
        </activity>
        <activity android:name="com.sendproperties.ws.Comercios" />
    </application>

</manifest>